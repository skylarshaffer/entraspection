{
  "manifest_version": 3,
  "name": "entraspection",
  "version": "1.0.0",
  "description": "",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "permissions": [
    "sidePanel",
    "tabs",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}